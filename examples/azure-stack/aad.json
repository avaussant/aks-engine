{
    "apiVersion": "vlabs",
    "location": "<LOCATION>",
    "properties": {
        "orchestratorProfile": {
            "orchestratorType": "Kubernetes",
            "orchestratorRelease": "1.11",
            "orchestratorVersion": "1.11.9",
            "kubernetesConfig": {
                "kubernetesImageBase": "msazurestackdocker/",
                "useInstanceMetadata": false,
                "networkPlugin": "flannel"
            }
        },
        "customCloudProfile": {
            "environment": {
                "name": "AzureStackCloud"
            },
            "identitySystem": "azure_ad",
            "authenticationMethod": "client_secret",
            "dependenciesLocation": "public",
            "portalUrl": "<PORTAL-URL>"
        },
        "masterProfile": {
            "dnsPrefix": "<DNS-PREFIX>",
            "distro": "<UBUNTU|AKS>",
            "count": 3,
            "osDiskSizeGB": 50,
            "vmSize": "Standard_D2_v2"
        },
        "agentPoolProfiles": [
            {
                "distro": "<UBUNTU|AKS>",
                "name": "linuxpool",
                "count": 3,
                "osDiskSizeGB": 50,
                "vmSize": "Standard_D2_v2",
                "availabilityProfile": "AvailabilitySet",
                "AcceleratedNetworkingEnabled": false
            }
        ],
        "linuxProfile": {
            "adminUsername": "azureuser",
            "ssh": {
                "publicKeys": [
                    {
                        "keyData": "<PUBLIC-KEY>"
                    }
                ]
            }
        },
        "servicePrincipalProfile": {
            "clientId": "<CONTRIBUTOR-SPN-on-TENANT-SUB>",
            "secret": "<SPN-SECRET>"
        }
    }
}